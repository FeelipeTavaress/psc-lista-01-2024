programa {
  cadeia nome
  inteiro idade
  real altura

  funcao inicio() {

  
    escreva ("qual seu nome? ")
    leia (nome)
    escreva(" \n qual sua idade? ")
    leia (idade)
    escreva(" \n qual sua altura? ")
    leia (altura)
    escreva("Nome: ", nome, "\n", "Idade:", idade, "\n", "altura:", altura)

  }
}
